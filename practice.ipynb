   "source": [
    "import tensorflow as tf\n",
    "\n",
    "input = tf.constant([1, 2, 3])  # 输入张量形状为 [3]\n",
    "multiples = [2]  # 在第一个维度上重复 2 次\n",
    "output = tf.tile(input, multiples)  # 复制和重复后的张量形状为 [6]\n",
    "\n",
    "with tf.Session() as sess:\n",
    "    result = sess.run(output)\n",
    "    print(result)"
   ]
